    <!-- JavaScript for fade-in animations -->
    <script>
        document.addEventListener('DOMContentLoaded', function () {
    // Function to check if an element is in the viewport with an offset to trigger earlier
    function isInViewport(element, offset = 200) { // offset in pixels; adjust as needed
        const rect = element.getBoundingClientRect();
        return (
            rect.top >= (0 - offset) &&
            rect.left >= (0 - offset) &&
            rect.bottom <= ((window.innerHeight || document.documentElement.clientHeight) + offset) &&
            rect.right <= ((window.innerWidth || document.documentElement.clientWidth) + offset)
        );
    }

    // Function to handle scroll events and trigger animations
    function handleScroll() {
        document.querySelectorAll('.fade-in').forEach(function (element) {
            if (isInViewport(element)) {
                element.classList.add('visible');
            }
        });
    }

    // Attach the handleScroll function to the scroll event
    window.addEventListener('scroll', handleScroll);

    // Trigger the initial animations on page load
    handleScroll();
});
    </script>

<script>
  function startAnimation() {
    // Define the tween from blob1 to blob2 and blob5 to blob6
    const tween1 = KUTE.fromTo('#blob1', { path: '#blob1' }, { path: '#blob2' }, { duration: 12000 });
    const tween5 = KUTE.fromTo('#blob5', { path: '#blob5' }, { path: '#blob6' }, { duration: 12000 });

    // Define the tween from blob2 to blob3 and blob6 to blob7
    const tween2 = KUTE.fromTo('#blob1', { path: '#blob2' }, { path: '#blob3' }, { duration: 12000 });
    const tween6 = KUTE.fromTo('#blob5', { path: '#blob6' }, { path: '#blob7' }, { duration: 12000 });

    // Define the tween from blob3 to blob4 and blob7 to blob8
    const tween3 = KUTE.fromTo('#blob1', { path: '#blob3' }, { path: '#blob4' }, { duration: 12000 });
    const tween7 = KUTE.fromTo('#blob5', { path: '#blob7' }, { path: '#blob8' }, { duration: 12000 });

    // Define the tween from blob4 back to blob1 and blob8 back to blob5
    const tween4 = KUTE.fromTo('#blob1', { path: '#blob4' }, { path: '#blob1' }, { duration: 12000, complete: function() { tween1.start(); } });
    const tween8 = KUTE.fromTo('#blob5', { path: '#blob8' }, { path: '#blob5' }, { duration: 12000, complete: function() { tween5.start(); } });


    // Setup looping by using the complete callback for both sequences
tween1.chain(tween2);
tween2.chain(tween3);
tween3.chain(tween4);
// The complete callback of tween4 already loops back to tween1
tween4.chain(tween1);
tween5.chain(tween6);
tween6.chain(tween7);
tween7.chain(tween8);
tween8.chain(tween5);
// The complete callback of tween8 already loops back to tween5

// Start the first tween of each sequence to kick off the simultaneous animations
tween1.start();
tween5.start();
  }

  // Start the animation sequences when the document is ready
  document.addEventListener('DOMContentLoaded', startAnimation);
</script>

