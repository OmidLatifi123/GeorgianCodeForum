    const tween4 = KUTE.fromTo('#blob1', { path: '#blob4' }, { path: '#blob1' }, { duration: 12000, complete: function() { tween1.start(); } });
    const tween8 = KUTE.fromTo('#blob5', { path: '#blob8' }, { path: '#blob5' }, { duration: 12000, complete: function() { tween5.start(); } });


    // Setup looping by using the complete callback for both sequences
tween1.chain(tween2);
tween2.chain(tween3);
tween3.chain(tween4);
// The complete callback of tween4 already loops back to tween1
tween4.chain(tween1);
tween5.chain(tween6);
tween6.chain(tween7);
tween7.chain(tween8);
tween8.chain(tween5);
// The complete callback of tween8 already loops back to tween5

// Start the first tween of each sequence to kick off the simultaneous animations
tween1.start();
tween5.start();
  }

  // Start the animation sequences when the document is ready
  document.addEventListener('DOMContentLoaded', startAnimation);
</script>

